
swap: 
PUSH BP
MOV BP, SP
MOV R0, 1
MOV R1, SP
ADD R0, R1
MOV SP, R0
MOV R1, BP
MOV R2, 3
SUB R1, R2
MOV R2, [R1]
MOV R1, R2
MOV R0 [R1]
MOV R1, BP
MOV R2, 1
ADD R1, R2
MOV [R1] R0
MOV R1, BP
MOV R2, 4
SUB R1, R2
MOV R2, [R1]
MOV R1, R2
MOV R0 [R1]
MOV R1, BP
MOV R2, 3
SUB R1, R2
MOV R2, [R1]
MOV R1, R2
MOV [R1] R0
MOV R1, BP
MOV R2, 1
ADD R1, R2
MOV R0 [R1]
MOV R1, BP
MOV R2, 4
SUB R1, R2
MOV R2, [R1]
MOV R1, R2
MOV [R1] R0
MOV R0, 1
MOV R1, BP
DCR R1
DCR R1
MOV [R1], R0
MOV SP, BP
POP R0
MOV BP, R0
RET



quicksort: 
PUSH BP
MOV BP, SP
MOV R0, 2
MOV R1, SP
ADD R0, R1
MOV SP, R0
MOV R1, BP
MOV R2, 4
SUB R1, R2
MOV R0 [R1]
MOV R2, BP
MOV R3, 5
SUB R2, R3
MOV R1 [R2]
LT R0 R1
JZ R0 l4
MOV R1, BP
MOV R2, 5
SUB R1, R2
MOV R0 [R1]
PUSH R0
MOV R1, BP
MOV R2, 4
SUB R1, R2
MOV R0 [R1]
PUSH R0
PUSH R0
CALL partition 
POP R0
POP R1
POP R1
MOV R1, BP
MOV R2, 1
ADD R1, R2
MOV [R1] R0
MOV R1, BP
MOV R2, 5
SUB R1, R2
MOV R0 [R1]
PUSH R0
MOV R1, BP
MOV R2, 1
ADD R1, R2
MOV R0 [R1]
MOV R1, 1
ADD R0 R1
PUSH R0
MOV R1, BP
MOV R2, 1
ADD R1, R2
MOV R0 [R1]
MOV R1, 1
SUB R0 R1
PUSH R0
MOV R1, BP
MOV R2, 4
SUB R1, R2
MOV R0 [R1]
PUSH R0
MOV R0 1
PUSH R0
PUSH R0
CALL quicksort 
POP R0
POP R1
POP R1
POP R1
PUSH R0
PUSH R0
CALL quicksort 
POP R0
POP R1
POP R1
POP R1
MOV R1, BP
MOV R2, 2
ADD R1, R2
MOV [R1] R0
l4: 
MOV R0 1
MOV R1, BP
DCR R1
DCR R1
MOV [R1], R0
MOV SP, BP
POP R0
MOV BP, R0
RET



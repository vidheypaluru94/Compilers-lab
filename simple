decl
integer i;
integer funct(integer b);
integer function(integer c);
enddecl

integer function(integer c) {
decl

enddecl
begin
write(c);
return 1;
end
}

integer funct(integer b) {
decl
integer a,c;
enddecl
begin
a=100;
c=30;
c = function(c);
write(b);
return a;
end
}

integer main() {
decl
integer b,c;
enddecl
begin
if(2>3) then
	c=10;
else 


	c = 20;

endif;
b = funct(c);
write(b);
return 1;
end
}
